-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Credit = Instance.new("TextLabel")
local DiddyUi = Instance.new("TextButton")
local DiddySpy = Instance.new("TextButton")
local AutoGay = Instance.new("TextButton")
local AntiMeat = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Workspace

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.575516939, 0, 0.230024219, 0)
Frame.Size = UDim2.new(0, 322, 0, 274)
Frame.Active = true
Frame.Draggable = true
UICorner.CornerRadius = UDim.new(0.100000001, 0)
UICorner.Parent = Frame

Credit.Name = "Credit"
Credit.Parent = Frame
Credit.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Credit.BorderColor3 = Color3.fromRGB(0, 0, 0)
Credit.BorderSizePixel = 0
Credit.Position = UDim2.new(0.189440995, 0, 0, 0)
Credit.Size = UDim2.new(0, 200, 0, 50)
Credit.Font = Enum.Font.Creepster
Credit.Text = "Diddy's Uncle"
Credit.TextColor3 = Color3.fromRGB(247, 0, 0)
Credit.TextScaled = true
Credit.TextSize = 14.000
Credit.TextWrapped = true

DiddyUi.Name = "Diddy Ui"
DiddyUi.Parent = Frame
DiddyUi.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
DiddyUi.BorderColor3 = Color3.fromRGB(0, 0, 0)
DiddyUi.BorderSizePixel = 0
DiddyUi.Position = UDim2.new(0.024844721, 0, 0.321167886, 0)
DiddyUi.Size = UDim2.new(0, 154, 0, 49)
DiddyUi.Font = Enum.Font.Creepster
DiddyUi.Text = "Diddy Ui"
DiddyUi.TextColor3 = Color3.fromRGB(247, 0, 0)
DiddyUi.TextSize = 27.000
DiddyUi.TextWrapped = true
DiddyUi.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Brxdy1/DiddyGui/refs/heads/main/Diddy%20Gui.txt"))()
end)
DiddySpy.Name = "Diddy Spy"
DiddySpy.Parent = Frame
DiddySpy.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
DiddySpy.BorderColor3 = Color3.fromRGB(0, 0, 0)
DiddySpy.BorderSizePixel = 0
DiddySpy.Position = UDim2.new(0.503105581, 0, 0.321167886, 0)
DiddySpy.Size = UDim2.new(0, 152, 0, 49)
DiddySpy.Font = Enum.Font.Creepster
DiddySpy.Text = "Diddy Spy"
DiddySpy.TextColor3 = Color3.fromRGB(247, 0, 0)
DiddySpy.TextSize = 27.000
DiddySpy.TextWrapped = true
DiddySpy.MouseButton1Down:connect(function()
	-- Services
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer

	-- Create the ScreenGui
	local ScreenGui = Instance.new("ScreenGui")
	ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
	ScreenGui.ResetOnSpawn = false

	-- Create the Frame
	local Frame = Instance.new("Frame")
	Frame.Parent = ScreenGui
	Frame.Size = UDim2.new(0, 300, 0, 200) -- Adjusted for the "Made By Brxdy" label
	Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
	Frame.AnchorPoint = Vector2.new(0.5, 0.5)
	Frame.Active = true
	Frame.BackgroundTransparency = 1 -- Start invisible

	-- Add a UIGradient for pink and blue gradient effect to the frame
	local FrameGradient = Instance.new("UIGradient")
	FrameGradient.Color = ColorSequence.new{
		ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 105, 180)), -- Pink
		ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 191, 255))  -- Deep Sky Blue
	}
	FrameGradient.Parent = Frame

	-- Draggable functionality
	local dragging, dragInput, dragStart, startPos

	local function updateInput(input)
		local delta = input.Position - dragStart
		Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	Frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = Frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	Frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			updateInput(input)
		end
	end)

	-- Create UICorner for rounded corners
	local UICorner = Instance.new("UICorner")
	UICorner.CornerRadius = UDim.new(0, 10) -- 10 pixel rounded corners
	UICorner.Parent = Frame

	-- Create the "Made By Brxdy" Label
	local Title = Instance.new("TextLabel")
	Title.Parent = Frame
	Title.Size = UDim2.new(1, 0, 0, 30)
	Title.Position = UDim2.new(0, 0, 0, 0)
	Title.Text = "Made By Brxdy"
	Title.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text
	Title.Font = Enum.Font.GothamBlack -- Clean, sharp font
	Title.TextScaled = true

	-- Add the same pink and blue gradient to the title
	local TitleGradient = FrameGradient:Clone()
	TitleGradient.Parent = Title

	-- Create the User TextBox
	local User = Instance.new("TextBox")
	User.Parent = Frame
	User.Size = UDim2.new(0, 200, 0, 50)
	User.Position = UDim2.new(0.5, -100, 0.2, 30)
	User.PlaceholderText = "Enter Username"
	User.Text = ""
	User.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text
	User.Font = Enum.Font.GothamBlack -- Clean, sharp font
	User.TextScaled = true
	User.BackgroundTransparency = 0.95 -- Highly transparent background
	User.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White background for contrast

	-- Create and add a gradient to the User TextBox
	local UserGradient = Instance.new("UIGradient")
	UserGradient.Color = ColorSequence.new{
		ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 105, 180)), -- Pink
		ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 191, 255))  -- Deep Sky Blue
	}
	UserGradient.Parent = User

	-- Create the Strength Label
	local Strength = Instance.new("TextLabel")
	Strength.Parent = Frame
	Strength.Size = UDim2.new(0, 200, 0, 50)
	Strength.Position = UDim2.new(0.5, -100, 0.6, 30)
	Strength.Text = "Strength: ???"
	Strength.TextColor3 = Color3.fromRGB(0, 0, 0) -- Black text
	Strength.Font = Enum.Font.GothamBlack -- Clean, sharp font
	Strength.TextScaled = true

	-- Add the same pink and blue gradient to the Strength label
	local StrengthGradient = FrameGradient:Clone()
	StrengthGradient.Parent = Strength

	-- UICorner for rounded corners on text elements
	local TextBoxCorner = Instance.new("UICorner")
	TextBoxCorner.CornerRadius = UDim.new(0, 10)
	TextBoxCorner.Parent = User

	local StrengthCorner = Instance.new("UICorner")
	StrengthCorner.CornerRadius = UDim.new(0, 10)
	StrengthCorner.Parent = Strength

	local TitleCorner = Instance.new("UICorner")
	TitleCorner.CornerRadius = UDim.new(0, 10)
	TitleCorner.Parent = Title

	-- Tween animations
	local function fadeInUI()
		local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local goal = {BackgroundTransparency = 0}
		local fadeTween = TweenService:Create(Frame, tweenInfo, goal)
		fadeTween:Play()

		local scaleTween = TweenService:Create(Frame, TweenInfo.new(1, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
			Size = UDim2.new(0, 300, 0, 200)
		})
		scaleTween:Play()
	end

	-- Hover animation for the TextBox and Strength labels
	local function hoverEffect(object)
		object.MouseEnter:Connect(function()
			TweenService:Create(object, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0, 220, 0, 55)
			}):Play()
		end)

		object.MouseLeave:Connect(function()
			TweenService:Create(object, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0, 200, 0, 50)
			}):Play()
		end)
	end

	hoverEffect(User)
	hoverEffect(Strength)

	-- Function to find player by partial username
	local function findPlayerByPartialName(partialName)
		for _, player in pairs(game.Players:GetPlayers()) do
			if string.sub(player.Name:lower(), 1, #partialName:lower()) == partialName:lower() then
				return player
			end
		end
		return nil
	end

	-- Script to update Strength
	local function updateStrength(player)
		if player then
			local data = game.ReplicatedStorage.Data:FindFirstChild(player.Name)
			if data and data:FindFirstChild("Strength") then
				Strength.Text = "Strength: " .. tostring(data.Strength.Value)

				-- Auto-update strength when it changes
				data.Strength.Changed:Connect(function(newStrength)
					Strength.Text = "Strength: " .. tostring(newStrength)
				end)
			else
				Strength.Text = "Strength data not found"
			end
		end
	end

	User.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local partialName = User.Text
			local player = findPlayerByPartialName(partialName)

			if player then
				-- Update the TextBox to show the full username
				User.Text = player.Name
				updateStrength(player)
			else
				Strength.Text = "Player not found"
			end
		end
	end)

	-- Chat notification with rainbow colors
	local function sendRainbowChatMessage(message)
		local colors = {
			Color3.fromRGB(255, 0, 0), -- Red
			Color3.fromRGB(255, 165, 0), -- Orange
			Color3.fromRGB(255, 255, 0), -- Yellow
			Color3.fromRGB(0, 255, 0), -- Green
			Color3.fromRGB(0, 0, 255), -- Blue
			Color3.fromRGB(75, 0, 130), -- Indigo
			Color3.fromRGB(238, 130, 238) -- Violet
		}

		for i, color in ipairs(colors) do
			game.StarterGui:SetCore("ChatMakeSystemMessage", {
				Text = message;
				Color = color;
			})
			wait(0.1)
		end
	end

	-- Send chat notification
	sendRainbowChatMessage("Thanks For Using Brxdy's Strength Spy!")

	-- Play fade-in animation when the GUI loads
	fadeInUI()
end)
AutoGay.Name = "Auto Gay"
AutoGay.Parent = Frame
AutoGay.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AutoGay.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoGay.BorderSizePixel = 0
AutoGay.Position = UDim2.new(0.0310559012, 0, 0.664233565, 0)
AutoGay.Size = UDim2.new(0, 152, 0, 49)
AutoGay.Font = Enum.Font.Creepster
AutoGay.Text = "Auto Gay"
AutoGay.TextColor3 = Color3.fromRGB(247, 0, 0)
AutoGay.TextSize = 27.000
AutoGay.TextWrapped = true
AutoGay.MouseButton1Down:connect(function()
	-- Gui to Lua
	-- Version: 3.2

	-- Instances:

	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local UIGradient = Instance.new("UIGradient")
	local TextButton = Instance.new("TextButton")

	--Properties:

	ScreenGui.Parent = game.CoreGui

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(242, 178, 253)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0.651567936, 0, 0.512106597, 0)
	Frame.Size = UDim2.new(0, 234, 0, 225)

	UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(26, 5, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 8, 255))}
	UIGradient.Parent = Frame

	TextButton.Parent = Frame
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.BackgroundTransparency = 1.000
	TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.BorderSizePixel = 0
	TextButton.Position = UDim2.new(0.260683775, 0, 0.195555553, 0)
	TextButton.Size = UDim2.new(0, 111, 0, 57)
	TextButton.Font = Enum.Font.LuckiestGuy
	TextButton.Text = "Start Dupe"
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextSize = 14.000

	-- Scripts:

	local function ZBBJTC_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)

		local isEnabled = false
		local function dupe(gamePassId)
			local market = game:GetService("MarketplaceService")
			local player = game.Players.LocalPlayer
			market:SignalPromptGamePassPurchaseFinished(player, gamePassId, true)

		end

		-- Function to handle duplication toggle
		script.Parent.MouseButton1Click:Connect(function()
			isEnabled = not isEnabled
			if isEnabled then
				script.Parent.Text = "Stop Duping Little Nig"
				print("Duplication started.")
				-- Start the duplication process
				while isEnabled do
					dupe(5949054)  -- Call the duplication function with the game pass ID
					wait(1)  -- Wait for 1 second before the next duplication
				end
			else
				script.Parent.Text = "Start Duping Jit"
				print("Duplication stopped.")
			end
		end)

	end
	coroutine.wrap(ZBBJTC_fake_script)()
	local function ICUVEHR_fake_script() -- Frame.LocalScript 
		local script = Instance.new('LocalScript', Frame)

		local UIS = game:GetService("UserInputService")
		local gui = script.Parent
		local dragging
		local dragInput
		local dragStart
		local startPos

		-- Function to update the position while dragging
		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(
				startPos.X.Scale,
				startPos.X.Offset + delta.X,
				startPos.Y.Scale,
				startPos.Y.Offset + delta.Y
			)
		end

		-- Mouse button down event (start dragging)
		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position

				-- Capture the movement input while dragging
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		-- Mouse move event (continue dragging)
		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				dragInput = input
			end
		end)

		-- Event to update position while dragging
		UIS.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	coroutine.wrap(ICUVEHR_fake_script)()
end)
AntiMeat.Name = "Anti Meat"
AntiMeat.Parent = Frame
AntiMeat.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AntiMeat.BorderColor3 = Color3.fromRGB(0, 0, 0)
AntiMeat.BorderSizePixel = 0
AntiMeat.Position = UDim2.new(0.503105581, 0, 0.664233565, 0)
AntiMeat.Size = UDim2.new(0, 152, 0, 49)
AntiMeat.Font = Enum.Font.Creepster
AntiMeat.Text = "Anti Meat"
AntiMeat.TextColor3 = Color3.fromRGB(247, 0, 0)
AntiMeat.TextSize = 27.000
AntiMeat.TextWrapped = true
AntiMeat.MouseButton1Down:connect(function()
	-- Anti-AFK Script with Draggable GUI
	local player = game.Players.LocalPlayer
	local screenGui = Instance.new("ScreenGui")
	local frame = Instance.new("Frame")
	local titleLabel = Instance.new("TextLabel")

	-- Configure ScreenGui
	screenGui.Parent = player:WaitForChild("PlayerGui")

	-- Configure Frame (smaller size)
	frame.Size = UDim2.new(0, 200, 0, 60)  -- Smaller frame size to fit the text
	frame.Position = UDim2.new(0.5, -100, 0.5, -30)  -- Centered position
	frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black background
	frame.BorderSizePixel = 0
	frame.Parent = screenGui
	frame.BackgroundTransparency = 0 -- 100% visible
	frame.Active = true

	-- Add rounded corners
	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0.1, 0)
	corner.Parent = frame

	-- Configure Title Label
	titleLabel.Size = UDim2.new(1, 0, 1, 0)  -- Fill the frame
	titleLabel.Position = UDim2.new(0, 0, 0, 0)
	titleLabel.BackgroundTransparency = 1
	titleLabel.Font = Enum.Font["Creepster"]
	titleLabel.Text = "Brxdy's Anti AFK Script"
	titleLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- Green color
	titleLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0) -- Black outline
	titleLabel.TextSize = 20
	titleLabel.Parent = frame

	-- Draggable Functionality
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	frame.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			update(input)
		end
	end)

	-- Anti-AFK Function
	local function keepAlive()
		while true do
			wait(1) -- Wait time set to 1 second
			game:GetService("VirtualUser"):CaptureController()
			game:GetService("VirtualUser"):ClickButton2(Vector2.new())
		end
	end

	-- Activate Notification
	game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
		Text = "You've just executed brxdy's anti afk roblox script";
		Color = Color3.fromRGB(0, 255, 0); -- Set the text color to green
		Font = Enum.Font["Creepster"];
		FontSize = Enum.FontSize.Size24;
	})

	-- Start Anti-AFK
	keepAlive()
end)
